{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "listenPort": {
      "type": "integer",
      "minimum": 0
    },
    "serveFrontPage": {
      "type": "boolean"
    },
    "serveSwagger": {
      "type": "boolean"
    },
    "taskSchedule": {
      "type": "string",
      "pattern": "^([0-5]?\\d|\\*)\\s([0-5]?\\d|\\*)\\s([0-1]?\\d|2[0-3]|\\*)\\s([1-9]|[12]\\d|3[01]|\\*)\\s([1-9]|1[0-2]|\\*)\\s([0-7]|\\*)$|^([0-5]?\\d|\\*)\\s([0-5]?\\d|\\*)\\s([0-1]?\\d|2[0-3]|\\*)\\s([1-9]|[12]\\d|3[01]|\\*)\\s([1-9]|1[0-2]|\\*)$",
      "minLength": 1
    },
    "postgreSQLBaseURI": {
      "type": "string",
      "pattern": "^postgres(?:ql)?://(?:(?:[a-zA-Z0-9._~!$&'()*+,;=%-]+)(?::[a-zA-Z0-9._~!$&'()*+,;=%-]+)?@)?(?:[a-zA-Z0-9.-]+|\\[[a-fA-F0-9:]+\\])(?::\\d+)?(?:/[a-zA-Z0-9._~!$&'()*+,;=%-]*)?(?:\\?[a-zA-Z0-9._~!$&'()*+,;=%-]+=[a-zA-Z0-9._~!$&'()*+,;=%-]+(?:&[a-zA-Z0-9._~!$&'()*+,;=%-]+=[a-zA-Z0-9._~!$&'()*+,;=%-]+)*)?$",
      "minLength": 1
    },
    "process": {
      "type": "integer",
      "minimum": 1
    },
    "thread": {
      "type": "integer",
      "minimum": 1
    },
    "styles": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "geojsons": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "datas": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "sprites": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "fonts": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "concurrency": {
      "type": "integer",
      "minimum": 1
    },
    "storeTransparent": {
      "type": "boolean"
    }
  },
  "required": ["id"]
}