{
  "type": "object",
  "properties": {
    "input": {
      "type": "object",
      "properties": {
        "ws": {
          "type": "object",
          "properties": {
            "clientID": {
              "type": "string",
              "minLength": 1
            },
            "requestID": {
              "type": "string",
              "minLength": 1
            },
            "interval": {
              "type": "integer",
              "minimum": 1
            },
            "event": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": ["clientID", "requestID", "interval", "event"]
        },
        "resolution": {
          "type": "array",
          "items": {
            "type": "number",
            "minimum": 0
          },
          "minItems": 2,
          "maxItems": 2
        }
      },
      "required": ["resolution"]
    },
    "preview": {
      "type": "object",
      "properties": {
        "format": {
          "type": "string",
          "enum": ["gif", "png", "jpg", "jpeg", "webp"]
        },
        "width": {
          "type": "integer",
          "minimum": 0
        },
        "height": {
          "type": "integer",
          "minimum": 0
        },
        "lineColor": {
          "type": "string",
          "minLength": 1
        },
        "lineWidth": {
          "type": "integer",
          "minimum": 0
        },
        "lineStyle": {
          "type": "string",
          "enum": ["solid", "dashed", "longDashed", "dotted", "dashedDot"]
        },
        "pageColor": {
          "type": "string",
          "minLength": 1
        },
        "pageSize": {
          "type": "integer",
          "minimum": 0
        },
        "pageFont": {
          "type": "string",
          "minLength": 1
        }
      }
    },
    "output": {
      "type": "object",
      "properties": {
        "alignContent": {
          "type": "object",
          "properties": {
            "horizontal": {
              "type": "string",
              "enum": ["head", "middle", "tail"]
            },
            "vertical": {
              "type": "string",
              "enum": ["head", "middle", "tail"]
            }
          }
        },
        "compression": {
          "type": "boolean"
        },
        "base64": {
          "type": "boolean"
        },
        "grayscale": {
          "type": "boolean"
        },
        "orientation": {
          "type": "string",
          "enum": ["portrait", "landscape"]
        },
        "paperSize": {
          "type": "array",
          "items": {
            "type": "number",
            "minimum": 0
          },
          "minItems": 2,
          "maxItems": 2
        }
      },
      "required": ["paperSize"]
    }
  },
  "required": ["input", "output"]
}