{
  "type": "object",
  "properties": {
    "ws": {
      "type": "object",
      "properties": {
        "clientID": {
          "type": "string",
          "minLength": 1
        },
        "requestID": {
          "type": "string",
          "minLength": 1
        },
        "event": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "clientID",
        "requestID",
        "event"
      ]
    },
    "base64": {
      "type": "boolean"
    },
    "grayscale": {
      "type": "boolean"
    },
    "format": {
      "type": "string",
      "enum": [
        "gif",
        "png",
        "jpg",
        "jpeg",
        "webp"
      ]
    },
    "zoom": {
      "type": "number",
      "minimum": 0,
      "maximum": 25
    },
    "extent": {
      "type": "array",
      "items": {
        "type": "number",
        "minimum": -180,
        "maximum": 180
      },
      "minItems": 4,
      "maxItems": 4
    },
    "bbox": {
      "type": "array",
      "items": {
        "type": "number",
        "minimum": -180,
        "maximum": 180
      },
      "minItems": 4,
      "maxItems": 4
    },
    "maxRendererPoolSize": {
      "type": "integer",
      "minimum": 1
    },
    "tileScale": {
      "type": "number",
      "minimum": 0
    },
    "tileSize": {
      "type": "integer",
      "enum": [
        256,
        512
      ]
    },
    "scheme": {
      "type": "string",
      "enum": [
        "tms",
        "xyz"
      ]
    },
    "styleJSON": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "version": {
          "type": "number"
        },
        "sources": {
          "type": "object"
        },
        "layers": {
          "type": "array"
        }
      }
    }
  },
  "anyOf": [
    {
      "required": [
        "zoom",
        "bbox",
        "styleJSON"
      ]
    },
    {
      "required": [
        "zoom",
        "extent",
        "styleJSON"
      ]
    }
  ]
}