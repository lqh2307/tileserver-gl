{
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "zoom": {
        "type": "number",
        "minimum": 0,
        "maximum": 25
      },
      "bbox": {
        "type": "array",
        "items": {
          "type": "number",
          "minimum": -180,
          "maximum": 180
        },
        "minItems": 4,
        "maxItems": 4
      },
      "tileScale": {
        "type": "number",
        "minimum": 0
      },
      "tileSize": {
        "type": "integer",
        "enum": [
          256,
          512
        ]
      },
      "styleJSON": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "version": {
            "type": "number"
          },
          "sources": {
            "type": "object"
          },
          "layers": {
            "type": "array"
          }
        }
      },
      "base64": {
        "type": "boolean"
      },
      "grayscale": {
        "type": "boolean"
      },
      "format": {
        "type": "string",
        "enum": [
          "gif",
          "png",
          "jpg",
          "jpeg",
          "webp"
        ]
      },
      "pitch": {
        "type": "number"
      },
      "bearing": {
        "type": "number"
      },
      "width": {
        "type": "integer",
        "minimum": 1
      },
      "height": {
        "type": "integer",
        "minimum": 1
      }
    },
    "required": [
      "styleJSON",
      "bbox",
      "zoom"
    ]
  },
  "minItems": 1
}