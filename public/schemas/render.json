{
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 1
    },
    "format": {
      "type": "string",
      "enum": ["gif", "png", "jpg", "jpeg", "webp"]
    },
    "zoom": {
      "type": "integer",
      "minimum": 0,
      "maximum": 25
    },
    "bbox": {
      "type": "array",
      "items": {
        "type": "number",
        "minimum": -180,
        "maximum": 180
      },
      "minItems": 4,
      "maxItems": 4
    },
    "maxRendererPoolSize": {
      "type": "integer",
      "minimum": 1
    },
    "concurrency": {
      "type": "integer",
      "minimum": 1
    },
    "storeTransparent": {
      "type": "boolean"
    },
    "tileSize": {
      "type": "integer",
      "enum": [256, 512]
    },
    "tileScale": {
      "type": "integer",
      "minimum": 1
    },
    "addFrame": {
      "type": "boolean"
    },
    "styleJSON": {
      "type": "object"
    }
  },
  "required": ["id", "format", "zoom", "bbox", "styleJSON"]
}