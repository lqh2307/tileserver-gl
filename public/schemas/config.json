{
  "type": "object",
  "properties": {
    "options": {
      "type": "object",
      "properties": {
        "listenPort": {
          "type": "integer",
          "minimum": 0
        },
        "serveFrontPage": {
          "type": "boolean"
        },
        "serveSwagger": {
          "type": "boolean"
        },
        "restartSchedule": {
          "type": "string",
          "pattern": "^((\\*|[0-5]?\\d|\\*\\/\\d+)\\s+)?(\\*|[0-5]?\\d|\\*\\/\\d+)\\s+(\\*|([01]?\\d|2[0-3])(,([01]?\\d|2[0-3]))*|\\*\\/\\d+)\\s+(\\*|[1-9]|[12]\\d|3[01]|\\*\\/\\d+)\\s+(\\*|1[0-2]|[1-9]|\\*\\/\\d+)\\s+(\\*|[0-7]|\\*\\/\\d+)$",
          "minLength": 1
        },
        "taskSchedule": {
          "type": "string",
          "pattern": "^((\\*|[0-5]?\\d|\\*\\/\\d+)\\s+)?(\\*|[0-5]?\\d|\\*\\/\\d+)\\s+(\\*|([01]?\\d|2[0-3])(,([01]?\\d|2[0-3]))*|\\*\\/\\d+)\\s+(\\*|[1-9]|[12]\\d|3[01]|\\*\\/\\d+)\\s+(\\*|1[0-2]|[1-9]|\\*\\/\\d+)\\s+(\\*|[0-7]|\\*\\/\\d+)$",
          "minLength": 1
        },
        "postgreSQLBaseURI": {
          "type": "string",
          "pattern": "^postgres(?:ql)?://(?:(?:[a-zA-Z0-9._~!$&'()*+,;=%-]+)(?::[a-zA-Z0-9._~!$&'()*+,;=%-]+)?@)?(?:[a-zA-Z0-9.-]+|\\[[a-fA-F0-9:]+\\])(?::\\d+)?(?:/[a-zA-Z0-9._~!$&'()*+,;=%-]*)?(?:\\?[a-zA-Z0-9._~!$&'()*+,;=%-]+=[a-zA-Z0-9._~!$&'()*+,;=%-]+(?:&[a-zA-Z0-9._~!$&'()*+,;=%-]+=[a-zA-Z0-9._~!$&'()*+,;=%-]+)*)?$",
          "minLength": 1
        },
        "process": {
          "type": "integer",
          "minimum": 1
        },
        "thread": {
          "type": "integer",
          "minimum": 1
        }
      }
    },
    "styles": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "style": {
            "type": "string",
            "minLength": 1
          },
          "bbox": {
            "type": "array",
            "items": {
              "type": "number",
              "minimum": -180,
              "maximum": 180
            },
            "minItems": 4,
            "maxItems": 4
          },
          "cache": {
            "type": "object",
            "properties": {
              "forward": {
                "type": "boolean"
              },
              "store": {
                "type": "boolean"
              }
            }
          },
          "validate": {
            "type": "boolean"
          }
        }
      }
    },
    "geojsons": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": {
          "type": "object",
          "properties": {
            "geojson": {
              "type": "string",
              "minLength": 1
            },
            "cache": {
              "type": "object",
              "properties": {
                "forward": {
                  "type": "boolean"
                },
                "store": {
                  "type": "boolean"
                }
              }
            },
            "validate": {
              "type": "boolean"
            }
          }
        }
      }
    },
    "datas": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "mbtiles": {
            "type": "string",
            "minLength": 1
          },
          "pmtiles": {
            "type": "string",
            "minLength": 1
          },
          "xyz": {
            "type": "string",
            "minLength": 1
          },
          "pg": {
            "type": "string",
            "minLength": 1
          },
          "cache": {
            "type": "object",
            "properties": {
              "forward": {
                "type": "boolean"
              },
              "store": {
                "type": "boolean"
              }
            }
          },
          "tilejson": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "description": {
                "type": "string"
              },
              "attribution": {
                "type": "string"
              },
              "version": {
                "type": "string"
              },
              "type": {
                "type": "string",
                "enum": [
                  "baselayer",
                  "overlay"
                ]
              },
              "scheme": {
                "type": "string",
                "enum": [
                  "tms",
                  "xyz"
                ]
              },
              "format": {
                "type": "string",
                "enum": [
                  "gif",
                  "png",
                  "jpg",
                  "jpeg",
                  "webp",
                  "pbf"
                ]
              },
              "minzoom": {
                "type": "integer",
                "minimum": 0,
                "maximum": 25
              },
              "maxzoom": {
                "type": "integer",
                "minimum": 0,
                "maximum": 25
              },
              "bounds": {
                "type": "array",
                "items": {
                  "type": "number",
                  "minimum": -180,
                  "maximum": 180
                },
                "minItems": 4,
                "maxItems": 4
              },
              "center": {
                "type": "array",
                "items": {
                  "type": "number",
                  "minimum": -180,
                  "maximum": 180
                },
                "minItems": 3,
                "maxItems": 3
              },
              "vector_layers": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "minzoom": {
                      "type": "integer",
                      "minimum": 0,
                      "maximum": 25
                    },
                    "maxzoom": {
                      "type": "integer",
                      "minimum": 0,
                      "maximum": 25
                    },
                    "fields": {
                      "type": "object",
                      "additionalProperties": {
                        "type": "string"
                      }
                    }
                  },
                  "required": [
                    "id"
                  ]
                },
                "minItems": 0
              },
              "tilestats": {
                "type": "object",
                "properties": {
                  "layerCount": {
                    "type": "integer"
                  }
                }
              }
            }
          },
          "validate": {
            "type": "boolean"
          }
        }
      }
    },
    "sprites": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "sprite": {
            "type": "string",
            "minLength": 1
          },
          "cache": {
            "type": "object",
            "properties": {
              "forward": {
                "type": "boolean"
              },
              "store": {
                "type": "boolean"
              },
              "validate": {
                "type": "boolean"
              }
            }
          }
        }
      }
    },
    "fonts": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "properties": {
          "font": {
            "type": "string",
            "minLength": 1
          },
          "cache": {
            "type": "object",
            "properties": {
              "forward": {
                "type": "boolean"
              },
              "store": {
                "type": "boolean"
              },
              "validate": {
                "type": "boolean"
              }
            }
          }
        }
      }
    }
  }
}